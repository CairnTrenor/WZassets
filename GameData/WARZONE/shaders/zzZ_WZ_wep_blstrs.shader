models/weapons/blstr_a280/a280_diff
{
	q3map_nolightmap
    {
        map models/weapons/blstr_a280/a280_diff
        rgbGen lightingDiffuse
    }
    {
        map models/weapons/blstr_a280/a280_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
} 

