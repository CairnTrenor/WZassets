// Always force a buffer of this radius around the object...
cityForcedBufferDistance0=2048.0
cityForcedBufferDistance1=1500.0
cityForcedBufferDistance2=1024.0
cityForcedBufferDistance3=1500.0
cityForcedBufferDistance4=1500.0
