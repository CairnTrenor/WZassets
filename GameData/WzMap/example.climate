// Only allow this type on slopes less steep then this...
cityForcedMaxAngle0=1.0
cityForcedMaxAngle1=6.0
cityForcedMaxAngle2=1.0
cityForcedMaxAngle3=1.0
cityForcedMaxAngle4=1.0
