// Force full solidity on this object (still culls tiny surfaces)...
cityForcedFullSolid0=0
cityForcedFullSolid1=1
cityForcedFullSolid2=1
cityForcedFullSolid3=1
cityForcedFullSolid4=1
cityForcedFullSolid5=1

overrideShader0=
