// Never place another of the same object within this radius...
cityForcedDistanceFromSame0=32768.0
cityForcedDistanceFromSame1=2048.0
cityForcedDistanceFromSame2=0
cityForcedDistanceFromSame3=0
cityForcedDistanceFromSame4=0
